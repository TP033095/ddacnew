@model IEnumerable<TP033095.Ship>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.shipName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.shipMaxLoad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fleet.fleetName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.shipName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.shipMaxLoad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fleet.fleetName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.shipId }) |
            @Html.ActionLink("Details", "Details", new { id=item.shipId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.shipId })
        </td>
    </tr>
}

</table>
