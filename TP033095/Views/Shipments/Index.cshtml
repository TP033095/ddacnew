        <th>
            @Html.DisplayNameFor(model => model.shipmentDestination)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.shipmentStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Yard.yardName)
