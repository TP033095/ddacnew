            @Html.DisplayFor(modelItem => item.shipmentDestination)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.shipmentStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Yard.yardName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Customer.customerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ship.shipName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.shipmentId }) |
